<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>

<GridPane styleClass="module-details-panel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
    </columnConstraints>
    <Label fx:id="setModuleDetails" wrapText="true" style="-fx-text-fill: white; -fx-font: bold 12pt Default;"/>
</GridPane>
